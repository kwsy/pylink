        bds.comm.topHijack = null;
        bds.comm.isDebug = false;
		
        
        
        
        
                                                                                                                                                            
        bds.comm.iaurl=["https:\/\/www.jb51.net\/books\/697580.html","https:\/\/www.bilibili.com\/video\/av36082351\/","https:\/\/www.aliyun.com\/citiao\/1572502.html"];

		bds.comm.curResultNum = "10";
