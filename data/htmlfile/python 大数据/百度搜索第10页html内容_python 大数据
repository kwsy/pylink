        try {
            bds.se.mon.srvt = parseInt(document.cookie.match(new RegExp("(^| )BDSVRTM=([^;]*)(;|$)"))[2]);
            document.cookie="BDSVRTM=;expires=Sat, 01 Jan 2000 00:00:00 GMT";
        }catch(e){
            bds.se.mon.srvt=-1;
        }

        bdUser        = bds.comm.user?bds.comm.user:null;
        bdQuery       = bds.comm.query;
